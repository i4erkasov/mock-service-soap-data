{
  "name": "i4erkasov/mock-soap-service",
  "config": {
    "sort-packages": true,
    "preferred-install": {
      "*": "dist"
    },
    "process-timeout": 1800
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-soap": "*",
    "doctrine/mongodb-odm": "*",
    "alcaeus/mongo-php-adapter": "^1.0.0",
    "guzzlehttp/guzzle": "^6.5",
    "guzzlehttp/psr7": "^1.6",
    "laminas/laminas-soap": "^2.8",
    "mongodb/mongodb": "^1.6",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.0",
    "php2wsdl/php2wsdl": "^0.6.1",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4",
    "vlucas/phpdotenv": "*",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
